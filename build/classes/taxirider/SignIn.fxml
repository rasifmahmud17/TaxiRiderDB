<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="taxirider.SignInController">
    <stylesheets>
        <URL value="@signin.css" />
    </stylesheets>
   <children>
      <HBox alignment="CENTER" prefHeight="59.0" prefWidth="600.0">
         <children>
            <Label text="SIGN IN">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="-1.0" layoutY="113.0" prefHeight="59.0" prefWidth="600.0">
         <children>
            <HBox alignment="CENTER_RIGHT" prefHeight="59.0" prefWidth="300.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <children>
                  <Label prefHeight="20.0" prefWidth="156.0" text="ID                     :">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="300.0">
               <children>
                  <TextField fx:id="SIID">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="-3.0" layoutY="169.0" prefHeight="59.0" prefWidth="600.0">
         <children>
            <HBox alignment="CENTER_RIGHT" prefHeight="59.0" prefWidth="300.0">
               <children>
                  <Label prefHeight="20.0" prefWidth="155.0" text="PASSWORD      :">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Label>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="300.0">
               <children>
                  <PasswordField fx:id="SIPassword">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </PasswordField>
               </children>
            </HBox>
         </children>
      </HBox>
      <Button fx:id="SISignInButt" layoutX="419.0" layoutY="325.0" mnemonicParsing="false" onAction="#SISignInButtClick" style="-fx-background-color: #252AC2;" text="SIGN IN" textFill="WHITE">
         <effect>
            <Reflection />
         </effect>
      </Button>
   </children>
</AnchorPane>
